{
  "name": "swappy",
	"displayName": "Swappy",
	"description": "Swap items around a comma",
	"publisher": "elethan",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"keywords": ["editing", "productivity", "swap", "swappy"],
	"activationEvents": [
        "onCommand:swappy.swap"
	],
	"icon": "images/icon.png",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "swappy.swap",
				"title": "Swap"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	}
}
